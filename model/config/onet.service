[Unit]
Description=MTCNN - oNet
After=syslog.target

[Service]
ExecStart=tensorflow_model_server --rest_api_port=8505 --model_name=onet --model_base_path=/etc/api-endpoint/resources/onet
User=www-data
Group=www-data
Restart=always
StandardError=syslog
NotifyAccess=all
KillSignal=SIGTERM

[Install]
WantedBy=multi-user.target